<?xml version="1.0"?>
<root>
  <appdef>
    <appname>TERMINAL</appname>
    <equal>com.apple.Terminal</equal>
  </appdef>
  <appdef>
    <appname>CHROME</appname>
    <equal>com.google.Chrome</equal>
  </appdef>
  <item>
    <name>Asus S400CA Latam Fixes</name>
    <appendix>This fixes provide a fully functiona Asus s400ca keyboard when the OS is configured with the Spanish set (NOT ISO)</appendix>
    <item>
        <name>Pipe fix</name>
        <identifier>s400ca.pipe_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::BACKQUOTE, ModifierFlag::NONE, KeyCode::7, ModifierFlag::OPTION_L</autogen>
    </item>

    <item>
        <name>Backquote fix</name>
        <identifier>s400ca.backquote_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::DANISH_DOLLAR, ModifierFlag::NONE, KeyCode::BACKQUOTE</autogen>
        <autogen>__KeyToKey__ KeyCode::DANISH_DOLLAR, ModifierFlag::SHIFT_L, KeyCode::BACKQUOTE, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::DANISH_DOLLAR, ModifierFlag::SHIFT_R, KeyCode::BACKQUOTE, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Opening question mark fix</name>
        <identifier>s400ca.opening_question_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::EQUAL, ModifierFlag::NONE, KeyCode::COMMA, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Closing question mark fix</name>
        <identifier>s400ca.closing_question_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::MINUS, ModifierFlag::SHIFT_L, KeyCode::DOT, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::MINUS, ModifierFlag::SHIFT_R, KeyCode::DOT, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Quote fix</name>
        <identifier>s400ca.quote_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::MINUS, ModifierFlag::NONE, KeyCode::BACKSLASH</autogen>
    </item>

    <item>
        <name>Open curly bracket fix</name>
        <identifier>s400ca.open_curly_bracket_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::QUOTE, ModifierFlag::NONE, KeyCode::BACKQUOTE, ModifierFlag::OPTION_L, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Close curly bracket fix</name>
        <identifier>s400ca.close_curly_bracket_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::BACKSLASH, ModifierFlag::NONE, KeyCode::BACKSLASH, ModifierFlag::OPTION_L, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Open squared brackets fix</name>
        <identifier>s400ca.open_squared_bracket_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::QUOTE, ModifierFlag::SHIFT_L, KeyCode::BACKQUOTE, ModifierFlag::OPTION_L</autogen>
        <autogen>__KeyToKey__ KeyCode::QUOTE, ModifierFlag::SHIFT_R, KeyCode::BACKQUOTE, ModifierFlag::OPTION_L</autogen>
    </item>

    <item>
        <name>Close squared brackets fix</name>
        <identifier>s400ca.close_squared_bracket_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::BACKSLASH, ModifierFlag::SHIFT_L, KeyCode::BACKSLASH, ModifierFlag::OPTION_L</autogen>
        <autogen>__KeyToKey__ KeyCode::BACKSLASH, ModifierFlag::SHIFT_R, KeyCode::BACKSLASH, ModifierFlag::OPTION_L</autogen>
    </item>

    <item>
        <name>Opening exclamation mark fix</name>
        <identifier>s400ca.opening_exclamation_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::EQUAL, ModifierFlag::SHIFT_L, KeyCode::1, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::EQUAL, ModifierFlag::SHIFT_R, KeyCode::1, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Closing exclamation mark fix</name>
        <identifier>s400ca.closing_exclamation_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::1, ModifierFlag::SHIFT_L, KeyCode::2, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::1, ModifierFlag::SHIFT_R, KeyCode::2, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Degree symbol fix</name>
        <identifier>s400ca.degree_symbol_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::BACKQUOTE, ModifierFlag::SHIFT_L, KeyCode::8, ModifierFlag::OPTION_L</autogen>
        <autogen>__KeyToKey__ KeyCode::BACKQUOTE, ModifierFlag::SHIFT_R, KeyCode::8, ModifierFlag::OPTION_L</autogen>
    </item>

    <item>
        <name>Circumflex accent fix</name>
        <identifier>s400ca.circumflex_accent_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::QUOTE, ModifierFlag::CONTROL_R, KeyCode::I, ModifierFlag::OPTION_L</autogen>
    </item>

    <item>
        <name>Grave accent fix</name>
        <identifier>s400ca.grave_accent_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::BACKSLASH, ModifierFlag::CONTROL_R, KeyCode::BRACKET_RIGHT</autogen>
    </item>

    <item>
        <name>Tilde fix</name>
        <identifier>s400ca.tilde_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::BRACKET_RIGHT, ModifierFlag::CONTROL_R, KeyCode::SEMICOLON, ModifierFlag::OPTION_L</autogen>
    </item>

    <item>
        <name>Backslash fix</name>
        <identifier>s400ca.backslash_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::MINUS, ModifierFlag::CONTROL_R, KeyCode::6, ModifierFlag::OPTION_L</autogen>
    </item>

    <item>
        <name>At key fix</name>
        <identifier>s400ca.at_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::Q, ModifierFlag::CONTROL_R, KeyCode::G, ModifierFlag::OPTION_L</autogen>
    </item>

    <item>
        <name>Dash fix</name>
        <identifier>s400ca.dash_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::SLASH, ModifierFlag::NONE, KeyCode::MINUS</autogen>
    </item>

    <item>
        <name>Plus fix</name>
        <identifier>s400ca.plus_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::BRACKET_RIGHT, ModifierFlag::NONE, KeyCode::EQUAL, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Ampersand fix</name>
        <identifier>s400ca.ampersand_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::6, ModifierFlag::SHIFT_L, KeyCode::7, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::6, ModifierFlag::SHIFT_R, KeyCode::7, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Slash fix</name>
        <identifier>s400ca.slash_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::7, ModifierFlag::SHIFT_L, KeyCode::6, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::7, ModifierFlag::SHIFT_R, KeyCode::6, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Opening Round bracket fix</name>
        <identifier>s400ca.opening_round_bracket_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::8, ModifierFlag::SHIFT_L, KeyCode::9, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::8, ModifierFlag::SHIFT_R, KeyCode::9, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Closing Round bracket fix</name>
        <identifier>s400ca.closing_round_bracket_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::9, ModifierFlag::SHIFT_L, KeyCode::0, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::9, ModifierFlag::SHIFT_R, KeyCode::0, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Equal fix</name>
        <identifier>s400ca.equal_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::0, ModifierFlag::SHIFT_L, KeyCode::EQUAL</autogen>
        <autogen>__KeyToKey__ KeyCode::0, ModifierFlag::SHIFT_R, KeyCode::EQUAL</autogen>
    </item>

    <item>
        <name>Double quote fix</name>
        <identifier>s400ca.double_quote_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::2, ModifierFlag::SHIFT_L, KeyCode::BACKSLASH, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::2, ModifierFlag::SHIFT_R, KeyCode::BACKSLASH, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Semicolon fix</name>
        <identifier>s400ca.semicolon_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::COMMA, ModifierFlag::SHIFT_L, KeyCode::QUOTE</autogen>
        <autogen>__KeyToKey__ KeyCode::COMMA, ModifierFlag::SHIFT_R, KeyCode::QUOTE</autogen>
    </item>

    <item>
        <name>Colon fix</name>
        <identifier>s400ca.colon_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::DOT, ModifierFlag::SHIFT_L, KeyCode::QUOTE, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::DOT, ModifierFlag::SHIFT_R, KeyCode::QUOTE, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Low dash fix</name>
        <identifier>s400ca.low_dash_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::SLASH, ModifierFlag::SHIFT_L, KeyCode::MINUS, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::SLASH, ModifierFlag::SHIFT_R, KeyCode::MINUS, ModifierFlag::SHIFT_L</autogen>
    </item>


    <item>
        <name>Asterisk fix</name>
        <identifier>s400ca.asterisk_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::BRACKET_RIGHT, ModifierFlag::SHIFT_L, KeyCode::8, ModifierFlag::SHIFT_L</autogen>
        <autogen>__KeyToKey__ KeyCode::BRACKET_RIGHT, ModifierFlag::SHIFT_R, KeyCode::8, ModifierFlag::SHIFT_L</autogen>
    </item>

    <item>
        <name>Terminal Fixes</name>
        <item>
            <name>Fix Ctrl+C</name>
            <only>TERMINAL</only>
            <identifier>s400ca.terminal.fixC</identifier>
            <autogen>__KeyToKey__ KeyCode::C, ModifierFlag::COMMAND_R, KeyCode::C, ModifierFlag::CONTROL_R</autogen>
            <autogen>__KeyToKey__ KeyCode::C, ModifierFlag::COMMAND_L, KeyCode::C, ModifierFlag::CONTROL_R</autogen>
        </item>
        <item>
            <name>Fix Ctrl+Z</name>
            <only>TERMINAL</only>
            <identifier>s400ca.terminal.fixZ</identifier>
            <autogen>__KeyToKey__ KeyCode::Z, ModifierFlag::COMMAND_R, KeyCode::Z, ModifierFlag::CONTROL_R</autogen>
            <autogen>__KeyToKey__ KeyCode::Z, ModifierFlag::COMMAND_L, KeyCode::Z, ModifierFlag::CONTROL_R</autogen>
        </item>
    </item>

    <item>
        <name>Alt+TAB navigation</name>
        <identifier>s400ca.alt_TAB_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::TAB, ModifierFlag::CONTROL_L, KeyCode::TAB, ModifierFlag::COMMAND_L</autogen>
        <autogen>__KeyToKey__ KeyCode::TAB, ModifierFlag::CONTROL_R, KeyCode::8, ModifierFlag::COMMAND_R</autogen>
    </item>

    <item>
        <name>Google Chrome F5 fix</name>
        <only>CHROME</only>
        <identifier>s400ca.chrome_f5_fix</identifier>
        <autogen>__KeyToKey__ KeyCode::F5, ModifierFlag::NONE, KeyCode::R, ModifierFlag::COMMAND_L</autogen>
    </item>

  </item>
</root>
